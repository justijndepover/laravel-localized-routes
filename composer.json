{
    "name": "justijndepover/laravel-localized-routes",
    "description": "Setup your Laravel application with localized routes",
    "license": "MIT",
    "authors": [
        {
            "name": "Justijn Depover",
            "email": "justijndepover@gmail.com"
        }
    ],
    "require": {},
    "require-dev": {
        "orchestra/testbench": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "Justijndepover\\LocalizedRoutes\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Justijndepover\\LocalizedRoutes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Justijndepover\\LocalizedRoutes\\LocalizedRoutesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
